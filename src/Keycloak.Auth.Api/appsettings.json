{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "AuthorizationUrl": "http://localhost:18080/realms/keycloak-auth-demo/protocol/openid-connect/auth"
  },
  "Authentication": {
    "MetadataAddress": "http://keycloak:8080/realms/keycloak-auth-demo/.well-known/openid-configuration",
    "ValidIssuer": "http://localhost:18080/realms/keycloak-auth-demo",
    "Audience": "account"
  }
}